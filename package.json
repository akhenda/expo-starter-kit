{
  "name": "expo-starter",
  "version": "1.0.0",
  "private": true,
  "volta": {
    "node": "16.19.1",
    "pnpm": "7.27.1"
  },
  "packageManager": "pnpm@7.27.1",
  "scripts": {
    "gc": "git add . && git-cz",
    "prepare": "chmod +x ./node_modules/husky/lib/bin.js && husky install",
    "start": "expo start",
    "start:clean": "expo start --clear",
    "start:dev": "expo start --dev-client",
    "start:dev:clean": "expo start --dev-client --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "generate": "plop",
    "test": "jest --watch --coverage=false --changedSince=origin/main --verbose --runInBand --detectOpenHandles",
    "test:debug": "jest -o --watch --coverage=false",
    "test:final": "jest",
    "test:update-snapshots": "jest -u --coverage=false",
    "test:ci": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettify:js": "prettier --write '**/*.js{,x}'",
    "prettify:json": "prettier --write '**/*.json'",
    "prettify:md": "prettier --write '**/*.md'",
    "prettify:ts": "prettier --write '**/*.ts{,x}'",
    "prettify": "npm-run-all prettify:*",
    "build:dev": "eas build --profile development",
    "build:dev-client": "eas build --profile dev-client",
    "build:prod": "eas build --profile production",
    "i18n:sync": "sync-i18n --files 'src/services/i18n/locales/*.json' --primary en --languages fr sw --space 2 --finalnewline true",
    "i18n:check": "pnpm run i18n:sync --check",
    "i18n:extract": "NODE_ENV=development babel -f ./babel.config.js 'src/**/*.{js,jsx,ts,tsx}' > /dev/null",
    "i18n:extract-and-sync": "pnpm run i18n:extract && pnpm run i18n:sync && git add src/services/i18n/locales",
    "reactotron": "adb reverse tcp:9090 tcp:9090 && open -a Reactotron"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css,json}": "prettier --write",
    "./src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@expo-google-fonts/coda": "^0.2.2",
    "@expo-google-fonts/ubuntu": "^0.2.2",
    "@expo/vector-icons": "^13.0.0",
    "@nandorojo/swr-firestore": "^0.16.0",
    "@react-native-async-storage/async-storage": "~1.17.11",
    "@react-native-community/blur": "^4.3.0",
    "@react-native-community/datetimepicker": "6.5.2",
    "@react-native-community/netinfo": "9.3.5",
    "@react-native-firebase/app": "^17.1.0",
    "@react-native-firebase/app-check": "^17.1.0",
    "@react-native-firebase/app-distribution": "^17.1.0",
    "@react-native-firebase/auth": "^17.1.0",
    "@react-native-firebase/crashlytics": "^17.1.0",
    "@react-native-firebase/dynamic-links": "^17.1.0",
    "@react-native-firebase/firestore": "^17.1.0",
    "@react-native-firebase/functions": "^17.2.0",
    "@react-native-firebase/perf": "^17.1.0",
    "@react-native-firebase/storage": "^17.1.0",
    "@react-native-picker/picker": "2.4.8",
    "@react-navigation/bottom-tabs": "^6.5.4",
    "@react-navigation/drawer": "^6.5.8",
    "@react-navigation/elements": "^1.3.14",
    "@react-navigation/native": "^6.1.3",
    "@react-navigation/native-stack": "^6.9.9",
    "@react-navigation/stack": "^6.3.12",
    "@udecode/zustood": "^1.1.3",
    "apisauce": "^2.1.6",
    "cerealizr": "1.0.1-alpha",
    "expo": "~47.0.12",
    "expo-asset": "~8.7.0",
    "expo-community-flipper": "^47.0.0",
    "expo-constants": "~14.0.2",
    "expo-dev-client": "~2.0.1",
    "expo-font": "~11.0.1",
    "expo-intent-launcher": "~10.3.1",
    "expo-linking": "~3.3.0",
    "expo-localization": "~14.0.0",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "expo-system-ui": "~2.0.1",
    "expo-updates": "~0.15.6",
    "expo-web-browser": "~12.0.0",
    "i18next": "^22.4.9",
    "lodash": "^4.17.21",
    "lottie-react-native": "5.1.4",
    "path-to-regexp": "^6.2.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-i18next": "^12.1.4",
    "react-native": "0.70.5",
    "react-native-flipper": "^0.177.0",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-logs": "^5.0.1",
    "react-native-mmkv": "2.5.1",
    "react-native-reanimated": "^2.12.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-shared-element": "0.8.4",
    "react-native-ui-lib": "^6.29.0",
    "react-native-web": "~0.18.9",
    "react-navigation-shared-element": "^3.1.3",
    "react-tracked": "^1.7.11",
    "swr": "^2.0.3",
    "usehooks-ts": "^2.9.1",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.12.9",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.1",
    "@testing-library/react-native": "^11.5.0",
    "@types/eslint": "^8.21.1",
    "@types/jest": "^29.2.5",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "babel-plugin-i18next-extract": "^0.9.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "commitizen": "^4.2.6",
    "eslint": "^8.32.0",
    "eslint-config-heimdall": "1.6.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "git-cz": "^4.9.0",
    "husky": "8.0.3",
    "i18next-json-sync": "^3.1.2",
    "jest": "29.0.0",
    "jest-expo": "~47.0.1",
    "jest-extended": "^3.2.4",
    "lint-staged": "^13.1.0",
    "msw": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "plop": "2.7.6",
    "prettier": "^2.8.3",
    "react-devtools-core": "^4.27.1",
    "react-native-mmkv-flipper-plugin": "^1.0.0",
    "react-test-renderer": "18.1.0",
    "reactotron-react-native": "^5.0.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
