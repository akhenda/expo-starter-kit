{
  "name": "expo-starter",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "18",
    "pnpm": "7",
    "npm": "Please Use PNPM instead of NPM",
    "yarn": "Please Use PNPM instead of Yarn"
  },
  "volta": {
    "node": "18.15.0",
    "pnpm": "7.29.1"
  },
  "packageManager": "pnpm@7.29.2",
  "scripts": {
    "gc": "git add . && git-cz",
    "prepare": "chmod +x ./node_modules/husky/lib/bin.js && husky install",
    "start": "expo start",
    "start:clean": "expo start --clear",
    "start:dev": "expo start --dev-client",
    "start:dev:clean": "expo start --dev-client --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "generate": "plop",
    "test": "jest --watch --coverage=false --changedSince=origin/main --verbose --runInBand --detectOpenHandles",
    "test:debug": "jest -o --watch --coverage=false",
    "test:final": "jest",
    "test:update-snapshots": "jest -u --coverage=false",
    "test:ci": "jest --ci --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettify:js": "prettier --write '**/*.js{,x}'",
    "prettify:json": "prettier --write '**/*.json'",
    "prettify:md": "prettier --write '**/*.md'",
    "prettify:ts": "prettier --write '**/*.ts{,x}'",
    "prettify:yaml": "prettier --write '**/*.y{,a}ml'",
    "prettify": "npm-run-all prettify:*",
    "eas-build-pre-install": "./tools/pre-install",
    "prebuild": "node ./tools/generate-easignore.js",
    "build:dev": "eas build --profile development",
    "build:dev-client": "eas build --profile dev-client",
    "build:preview": "eas build --profile preview",
    "build:rc": "eas build --profile rc",
    "build:prod": "eas build --profile production",
    "i18n:sync": "sync-i18n --files 'src/services/i18n/locales/*.json' --primary en --languages fr sw --space 2 --finalnewline true",
    "i18n:check": "pnpm run i18n:sync --check",
    "i18n:extract": "NODE_ENV=development babel -f ./babel.config.js 'src/**/*.{js,jsx,ts,tsx}' > /dev/null",
    "i18n:extract-and-sync": "pnpm run i18n:extract && pnpm run i18n:sync && git add src/services/i18n/locales",
    "reactotron": "adb reverse tcp:9090 tcp:9090 && open -a Reactotron"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css,json}": "prettier --write",
    "./src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix --cache"
    ]
  },
  "dependencies": {
    "@expo-google-fonts/coda": "0.2.3",
    "@expo-google-fonts/ubuntu": "0.2.3",
    "@expo/vector-icons": "13.0.0",
    "@nandorojo/swr-firestore": "0.16.0",
    "@react-native-async-storage/async-storage": "~1.17.11",
    "@react-native-community/blur": "4.3.0",
    "@react-native-community/datetimepicker": "6.7.5",
    "@react-native-community/netinfo": "9.3.7",
    "@react-native-firebase/app": "17.3.2",
    "@react-native-firebase/auth": "17.3.2",
    "@react-native-firebase/crashlytics": "17.3.2",
    "@react-native-firebase/dynamic-links": "17.3.2",
    "@react-native-firebase/firestore": "17.3.2",
    "@react-native-firebase/functions": "17.3.2",
    "@react-native-firebase/perf": "^17.3.2",
    "@react-native-firebase/storage": "17.3.2",
    "@react-native-masked-view/masked-view": "github:react-native-masked-view/masked-view",
    "@react-native-picker/picker": "2.4.8",
    "@react-navigation/bottom-tabs": "6.5.7",
    "@react-navigation/drawer": "6.6.2",
    "@react-navigation/elements": "1.3.17",
    "@react-navigation/native": "6.1.6",
    "@react-navigation/native-stack": "6.9.12",
    "@react-navigation/stack": "6.3.16",
    "@udecode/zustood": "1.1.3",
    "apisauce": "2.1.6",
    "cerealizr": "1.0.1-alpha",
    "color": "4.2.3",
    "dotenv": "16.0.3",
    "expo": "~47.0.12",
    "expo-asset": "~8.9.0",
    "expo-build-properties": "~0.5.0",
    "expo-community-flipper": "47.0.2",
    "expo-constants": "~14.2.0",
    "expo-dev-client": "~2.1.0",
    "expo-font": "~11.1.0",
    "expo-intent-launcher": "~10.5.0",
    "expo-linking": "~3.3.1",
    "expo-localization": "~14.1.0",
    "expo-splash-screen": "~0.18.0",
    "expo-status-bar": "~1.4.2",
    "expo-system-ui": "~2.2.0",
    "expo-updates": "~0.16.0",
    "expo-web-browser": "~12.1.0",
    "firebase": "^9.17.2",
    "i18next": "22.4.11",
    "lodash": "4.17.21",
    "lottie-react-native": "5.1.5",
    "path-to-regexp": "6.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "12.2.0",
    "react-native": "0.71.4",
    "react-native-flipper": "0.184.0",
    "react-native-gesture-handler": "2.9.0",
    "react-native-iphone-x-helper": "1.3.1",
    "react-native-logs": "5.0.1",
    "react-native-mmkv": "2.6.3",
    "react-native-reanimated": "2.14.4",
    "react-native-redash": "18.1.0",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-shared-element": "0.8.8",
    "react-native-svg": "13.8.0",
    "react-native-ui-lib": "7.2.1",
    "react-native-web": "~0.18.9",
    "react-navigation-shared-element": "3.1.3",
    "react-tracked": "1.7.11",
    "swr": "2.1.0",
    "usehooks-ts": "2.9.1",
    "zustand": "4.3.6"
  },
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/core": "7.21.0",
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@react-native-community/eslint-config": "3.2.0",
    "@semantic-release/changelog": "6.0.2",
