{
  "expo": {
    "name": "expo-starter",
    "slug": "expo-starter-kit",
    "version": "1.0.0",
    "description": "My Expo Starter Kit. Highly opinionated. Use at own risk.",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "expo-starter",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/9cee5153-b650-4374-b3ce-a92218e67978"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.akhenda.expostarter",
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#194351"
      },
      "package": "com.akhenda.expostarter",
      "googleServicesFile": "./google-services.json",
      "versionCode": 0
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9cee5153-b650-4374-b3ce-a92218e67978"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "plugins": [
      "expo-community-flipper",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/crashlytics",
      "@react-native-firebase/perf",
      "@react-native-firebase/dynamic-links",
      [
        "expo-build-properties",
        {
          "ios": {
            "flipper": true
          }
        }
      ]
    ]
  }
}
